# csync2-iWatcher configuration
#
# **** DO NOT EDIT THIS FILE ****
#
# This file is managed by Salt.
# Any changes will be overwritten.

nossl * *;
group {{ pillar['csync2']['group_name'] }}
{
    {% for host in pillar['csync2']['hosts'] %}
    host {{ host }};
    {% endfor %}
    {% for folder in pillar['csync2']['folders'] %}
    include {{folder}};
    {% endfor %}
    key {{ pillar['csync2']['key_loc'] }};
}
